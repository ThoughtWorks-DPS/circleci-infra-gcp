{
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "(^|/)([Dd]ocker|[Cc]ontainer)file[^/]*$"
      ],
      "matchStrings": [
        "#\\s*renovate:\\s*datasource=(?<datasource>.*?) depName=(?<depName>.*?)\\s?(versioning=(?<versioning>.*?))?$"
      ],
      "versioningTemplate": "{{#if versioning}}{{{versioning}}}{{else}}semver{{/if}}"
    }
  ],
  "branchPrefix": "renovate-test-",
  "branchPrefixOld": "renovate-first-test-"
}
